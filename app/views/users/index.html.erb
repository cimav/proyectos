<nav class="config-breadcrumb z-depth-0">
    <div class="nav-wrapper">
      <div class="col s12">
        <a href="/configuracion" class="breadcrumb">Configuraci√≥n</a>
        <a href="/configuracion/usuarios" class="breadcrumb">Usuarios</a>
      </div>
